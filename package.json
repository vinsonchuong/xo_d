{
  "name": "@vinsonchuong/xo_d",
  "version": "0.0.0",
  "description": "Speed up xo in the same way as eslint_d",
  "keywords": [],
  "homepage": "https://github.com/vinsonchuong/xo_d",
  "bugs": "https://github.com/vinsonchuong/xo_d/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "repository": "vinsonchuong/xo_d",
  "scripts": {
    "test": "xo && ava",
    "release": "semantic-release"
  },
  "type": "commonjs",
  "bin": {
    "xo_d": "cli.js"
  },
  "dependencies": {
    "core_d": "^5.0.1",
    "eslint-formatter-pretty": "^5.0.0",
    "get-stdin": "^9.0.0",
    "meow": "^12.0.0",
    "semver": "^7.3.8",
    "xo": "^0.56.0"
  },
  "devDependencies": {
    "ava": "^5.0.1",
    "ava-patterns": "^3.2.0",
    "get-stream": "^8.0.0",
    "semantic-release": "^21.0.0"
  },
  "ava": {
    "verbose": true
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "semicolon": false,
    "rules": {
      "complexity": "off",
      "import/no-anonymous-default-export": "off",
      "unicorn/no-process-exit": "off",
      "unicorn/prefer-module": "off",
      "unicorn/prefer-top-level-await": "off"
    }
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
