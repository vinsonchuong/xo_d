{
  "name": "@vinsonchuong/xo_d",
  "version": "0.0.0",
  "description": "Speed up xo in the same way as eslint_d",
  "keywords": [],
  "homepage": "https://github.com/vinsonchuong/xo_d",
  "bugs": "https://github.com/vinsonchuong/xo_d/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "repository": "vinsonchuong/xo_d",
  "scripts": {
    "test": "xo && ava",
    "release": "semantic-release"
  },
  "type": "commonjs",
  "bin": "cli.js",
  "dependencies": {
    "core_d": "^6.1.0",
    "eslint-formatter-pretty": "^6.0.1",
    "find-cache-directory": "^6.0.0",
    "get-stdin": "^9.0.0",
    "meow": "^13.2.0",
    "path-exists": "^5.0.0",
    "xo": "^1.1.0"
  },
  "devDependencies": {
    "ava": "^6.4.0",
    "ava-patterns": "^3.2.1",
    "get-stream": "^9.0.1",
    "semantic-release": "^24.2.5",
    "typescript": "^5.8.3"
  },
  "ava": {
    "verbose": true
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "semicolon": false,
    "rules": {
      "complexity": "off",
      "import/no-anonymous-default-export": "off",
      "unicorn/no-process-exit": "off",
      "unicorn/prefer-module": "off",
      "unicorn/prefer-top-level-await": "off"
    }
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "yarn@4.9.2"
}
